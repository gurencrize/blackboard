    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
				"comment",
				"punctuation.definition.comment"
			],
            "settings": {
                "foreground": "#b4c6ae"
            }
		},
        {
            "name": "Variables",
            "scope": [
				"variable",
				"string constant.other.placeholder"
			],
            "settings": {
                "foreground": "#819FF7"
            }
		},
        {
            "name": "Colors",
            "scope": [
				"constant.other.color"
			],
            "settings": {
                "foreground": "#CCDCCC"
            }
		},
        {
            "name": "Invalid",
            "scope": [
				"invalid",
				"invalid.illegal"
			],
            "settings": {
                "foreground": "#CC7666"
            }
		},
        {
            "name": "Keyword, Storage",
            "scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
            "settings": {
                "foreground": "#A1CE71"
            }
		},
        {
            "name": "Operator, Misc",
            "scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
            "settings": {
                "foreground": "#CCA866"
            }
		},
        {
            "name": "Tag",
            "scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
            "settings": {
                "foreground": "#CC7666"
            }
		},
        {
            "name": "Function, Special Method",
            "scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
            "settings": {
                "foreground": "#819FF7"
            }
		},
        {
            "name": "Block Level Variables",
            "scope": [
				"meta.block variable.other"
			],
            "settings": {
                "foreground": "#CC7666"
            }
		},
        {
            "name": "Other Variable, String Link",
            "scope": [
				"support.other.variable",
				"string.other.link"
			],
            "settings": {
                "foreground": "#CC7666"
            }
		},
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
            "settings": {
                "foreground": "#CCC58E"
            }
		},
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
            "settings": {
                "foreground": "#A1CE71"
            }
		},
        {
            "name": "Class, Support",
            "scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
